<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">


    <display-name>Archetype Created Web Application</display-name>

    <welcome-file-list>

        <welcome-file>web1/index.jsp</welcome-file>

    </welcome-file-list>

    <servlet>
        <servlet-name>Start</servlet-name>
        <servlet-class>by.gsu.epamlab.Servlets.Start</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Start</servlet-name>
        <url-pattern>/start</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Other</servlet-name>
        <servlet-class>by.gsu.epamlab.Servlets.Other</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Other</servlet-name>
        <url-pattern>/other</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>by.gsu.epamlab.Servlets.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>by.gsu.epamlab.Servlets.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateUser</servlet-name>
        <servlet-class>by.gsu.epamlab.Servlets.CreateUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateUser</servlet-name>
        <url-pattern>/createUser</url-pattern>
    </servlet-mapping>

</web-app>

